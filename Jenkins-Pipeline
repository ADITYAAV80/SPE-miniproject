pipeline {
    agent any

    stages {
        stage('Clone Repository') {
            steps {
                git 'https://github.com/ADITYAAV80/SPE-miniproject.git'
            }
        }

        stage('Run Tests') {
            steps {
                sh 'mvn test'
            }
        }

        stage('Build') {
              steps {
                sh 'mvn clean package'
              }
        }

    }
